{
  "presets": ["env", "react", "stage-1"],
  "plugins": [
    "transform-react-jsx",
    "transform-flow-strip-types",
    "transform-class-properties",
    ["import", { "libraryName": "antd" }]
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "livereactload/babel-transform",
            "imports": ["react"]
          }]
        }]
      ]
    }
  }
}
